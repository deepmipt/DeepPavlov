{
  "chainer": {
    "in": [
      "question_raw"
    ],
    "out": [
      "best_answer"
    ],
    "pipe": [
      {
        "name": "tfidf_ranker",
        "in": [
          "question_raw"
        ],
        "out": [
          "doc_ids",
          "doc_scores"
        ],
        "fit_on_batch": [
          "questions_raw"
        ],
        "vectorizer": {
          "name": "hashing_tfidf_vectorizer",
          "fit_on_batch": [
            "questions_raw"
          ],
          "save_path": "odqa/ruwiki_test_tfidf.npz",
          "load_path": "odqa/ruwiki_test_tfidf.npz",
          "tokenizer": {
            "name": "ru_tokenizer",
            "lemmas": true,
            "ngram_range": [
              1,
              2
            ]
          }
        }
      },
      {
        "name": "wiki_sqlite_vocab",
        "in": [
          "doc_ids"
        ],
        "out": [
          "context_raw"
        ],
        "join_docs": false,
        "shuffle": false,
        "load_path": "odqa/ruwiki_test.db"
      },
      {
        "name": "document_chunker",
        "in": [
          "context_raw"
        ],
        "out": [
          "chunks"
        ],
        "flatten_result": true
      },
      {
        "name": "string_multiplier",
        "in": [
          "question_raw",
          "chunks"
        ],
        "out": [
          "questions"
        ]
      },
      {
        "name": "logit_ranker",
        "squad_model": {
          "config_path": "../deeppavlov/configs/squad/ru_squad.json"
        },
        "in": [
          "chunks",
          "questions"
        ],
        "out": [
          "best_answer"
        ]
      }
    ]
  },
  "metadata": {
    "requirements": [],
    "labels": {
      "server_utils": "ODQA"
    },
    "download": [
      "http://files.deeppavlov.ai/deeppavlov_data/odqa_test.tar.gz"
    ]
  }
}