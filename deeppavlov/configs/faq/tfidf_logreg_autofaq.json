{
  "dataset_reader": {
    "name": "faq_reader",
    "x_col_name": "Question",
    "y_col_name": "Answer",
    "data_url": "http://files.deeppavlov.ai/faq/school/faq_school.csv"
  },
  "dataset_iterator": {
    "name": "data_learning_iterator"
  },
  "chainer": {
    "in": "q",
    "pipe": [
      {
        "id": "classes_vocab",
        "name": "simple_vocab",
        "fit_on": [
          "y"
        ],
        "level": "token",
        "save_path": "vocabs/snips_classes.dict",
        "load_path": "vocabs/snips_classes.dict",
        "in": "y",
        "out": "y_ids"
      },
      {
        "name": "ru_tokenizer",
        "id": "my_tokenizer",
        "in": "q",
        "lemmas": true,
        "out": "q_token_lemmas"
      },
      {
        "ref": "my_tokenizer",
        "in": "q_token_lemmas",
        "out": "q_lem"
      },
      {
        "in": [
          "q_lem"
        ],
        "out": [
          "q_vect"
        ],
        "fit_on": [
          "q_lem"
        ],
        "id": "tfidf_vec",
        "name": "sklearn_component",
        "save_path": "vectorizer/tfidf_vectorizer_ruwiki.pkl",
        "load_path": "vectorizer/tfidf_vectorizer_ruwiki.pkl",
        "model_class": "sklearn.feature_extraction.text:TfidfVectorizer",
        "infer_method": "transform"
      },
      {
        "in": "q_vect",
        "fit_on": [
          "q_vect",
          "y"
        ],
        "out": [
          "answer"
        ],
        "C": 1000,
        "penalty": "l2",
        "name": "sklearn_component",
        "main": true,
        "save_path": "faq/tfidf_logreg_classifier.pkl",
        "load_path": "faq/tfidf_logreg_classifier.pkl",
        "model_class": "sklearn.linear_model:LogisticRegression",
        "infer_method": "predict"
      }
    ],
    "out": [
      "answer"
    ]
  },
  "train": {
    "validate_best": false,
    "test_best": false
  },
  "metadata": {
    "download": [
      {
        "url": "http://files.deeppavlov.ai/faq/school/tfidf_logreg_classifier.pkl",
        "subdir": "faq"
      },
      {
        "url": "http://files.deeppavlov.ai/vectorizer/tfidf_vectorizer_ruwiki.pkl",
        "subdir": "vectorizer"
      }
    ]
  }
}



