{
  "chainer": {
    "in": ["text"],
    "out": ["response"],
    "pipe": [
      {
        "name": "slotfill_raw",
        "in": ["text"],
        "out": ["slots"],
        "return_all": true,
        "load_path": "../deeppavlov/configs/sbrf_bot/slots.tarifs.json",
        "save_path": "../deeppavlov/configs/sbrf_bot/slots.tarifs.json"
      },
      {
        "name": "simple_dst",
        "id": "dst",
        "in": ["slots"],
        "out": ["state"],
        "commands": "./deeppavlov/configs/sbrf_bot/dst.tarifs.py"
      },
      {
        "name": "simple_policy",
        "dst": "#dst",
        "policy": "./deeppavlov/configs/sbrf_bot/policy.tarifs.py",
        "in": ["state"],
        "out": ["response"]
      }
    ]
  }

}
