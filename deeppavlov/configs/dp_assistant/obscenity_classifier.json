{
    "chainer": {
        "in": [
            "dialogs"
        ],
        "pipe": [
            {
                "class_name": "dialogs_parser",
                "in": [
                    "dialogs"
                ],
                "out": [
                    "last_utterances",
                    "last_annotations",
                    "utterances_histories",
                    "annotations_histories",
                    "dialog_ids",
                    "user_ids"
                ]
            },
            {
                "class_name": "obscenity_classifier",
                "data_path": "{DOWNLOADS_PATH}/obscenity_dataset/",
                "in": "last_utterances",
                "out": "obscenity_or_not"
            }
        ],
        "out": [
            "obscenity_or_not"
        ]
    },
    "metadata": {
        "variables": {
          "ROOT_PATH": "~/.deeppavlov",
          "DOWNLOADS_PATH": "{ROOT_PATH}/downloads"
        },
        "download": [
            {
              "url": "http://files.deeppavlov.ai/models/obscenity_classifier/ru_obscenity_dataset.zip",
              "subdir": "{DOWNLOADS_PATH}/obscenity_dataset"
            }
          ]
      }
}